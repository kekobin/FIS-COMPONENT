<div class="rookie J_rookie" data-channel="<%=channel%>" data-livechannel="<%=liveChannel%>">
    <div class="rookie-pic">
        <img src="<%=avatar%>" onerror="this.onerror=null; this.src='http://assets.dwstatic.com/amkit/p/duya/common/img/default_profile.jpg'" />
        <span class="m-mask"><i></i></span>
    </div>
    <div class="rookie-infoWrap">
        <div class="rookie-info">
            <p class="rookie-game"><%-gameFullName%></p>
            <p class="rookie-nick" title="<%-nick%>"><%-nick%></p>
        </div>
    </div>
</div>
